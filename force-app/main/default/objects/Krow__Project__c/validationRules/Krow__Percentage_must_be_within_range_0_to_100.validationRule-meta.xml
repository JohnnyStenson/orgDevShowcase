<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Krow__Percentage_must_be_within_range_0_to_100</fullName>
    <active>true</active>
    <description>Percentage complete must be within range 0 to 100.</description>
