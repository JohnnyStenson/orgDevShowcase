<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>SBQQ__NetTotal__c</fullName>
    <deprecated>false</deprecated>
    <description>Net total price for this quote line item.</description>
    <externalId>false</externalId>
    <formula>IF(SBQQ__Renewal__c, 0, SBQQ__NetPrice__c * SBQQ__EffectiveQuantity__c)</formula>
    <inlineHelpText>Net total price for this quote line item.</inlineHelpText>
    <label>Net Total</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
