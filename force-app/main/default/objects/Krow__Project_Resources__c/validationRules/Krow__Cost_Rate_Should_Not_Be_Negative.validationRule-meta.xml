<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Krow__Cost_Rate_Should_Not_Be_Negative</fullName>
    <active>true</active>
    <errorConditionFormula>AND(NOT(ISNULL(Krow__Cost_Rate__c)),Krow__Cost_Rate__c &lt; 0)</errorConditionFormula>
    <errorMessage>Cost Rate should not be negative.</errorMessage>
</ValidationRule>
