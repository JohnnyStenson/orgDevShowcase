<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>analyticsengine__Retry_If_Failed__c</fullName>
    <deprecated>false</deprecated>
    <externalId>false</externalId>
    <inlineHelpText>If the report fails, we can retry. The first retry will be immediate, subsequent retries will be spaced an hour apart. This helps if the report gets a Read Time Out error sometimes.</inlineHelpText>
    <label>Retry If Failed</label>
    <required>false</required>
    <trackTrending>false</trackTrending>
    <type>Picklist</type>
    <valueSet>
        <restricted>true</restricted>
        <valueSetDefinition>
            <sorted>false</sorted>
            <value>
                <fullName>No Retry</fullName>
                <default>false</default>
                <label>No Retry</label>
            </value>
            <value>
                <fullName>Retry Immediately</fullName>
                <default>false</default>
                <label>Retry Immediately</label>
            </value>
            <value>
                <fullName>Retry Two Times</fullName>
                <default>false</default>
                <label>Retry Two Times</label>
            </value>
            <value>
                <fullName>Retry Three Times</fullName>
                <default>false</default>
                <label>Retry Three Times</label>
            </value>
            <value>
                <fullName>Retry Four Times</fullName>
                <default>false</default>
                <label>Retry Four Times</label>
            </value>
            <value>
                <fullName>Retry Five Times</fullName>
                <default>false</default>
                <label>Retry Five Times</label>
            </value>
        </valueSetDefinition>
    </valueSet>
</CustomField>
