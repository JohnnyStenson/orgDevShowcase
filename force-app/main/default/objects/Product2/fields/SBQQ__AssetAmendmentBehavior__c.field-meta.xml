    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Picklist</type>
    <valueSet>
        <valueSetDefinition>
            <sorted>false</sorted>
            <value>
                <fullName>Default</fullName>
                <default>true</default>
                <label>Default</label>
            </value>
            <value>
                <fullName>Allow Refund</fullName>
                <default>false</default>
                <label>Allow Refund</label>
            </value>
        </valueSetDefinition>
    </valueSet>
</CustomField>
