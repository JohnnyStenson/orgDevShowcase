<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Krow__Resource_or_Role_is_required</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISBLANK( Krow__Project_Resource__c ),ISBLANK(  Krow__Role__c ))</errorConditionFormula>
    <errorMessage>Either Resource or Role field must be filled</errorMessage>
</ValidationRule>
