<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>SBQQ__LineNumbering__c</fullName>
    <deprecated>false</deprecated>
    <description>Specifies how line items are numbered on generated document.</description>
    <externalId>false</externalId>
    <inlineHelpText>Specify how to number line items on generated documents. &quot;Start at quote&quot; numbers all lines sequentially across groups (if any). &quot;Start at group&quot; resets numbers within each group.</inlineHelpText>
    <label>Line Numbering</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Picklist</type>
    <valueSet>
        <valueSetDefinition>
            <sorted>false</sorted>
            <value>
                <fullName>Start at quote</fullName>
                <default>true</default>
                <label>Start at quote</label>
            </value>
            <value>
                <fullName>Start at group</fullName>
                <default>false</default>
                <label>Start at group</label>
            </value>
        </valueSetDefinition>
    </valueSet>
</CustomField>
