    <formulas>
        <name>start_time_extracted_minus_10min_due</name>
        <dataType>String</dataType>
        <expression>IF(
  OR(
    VALUE( MID( TEXT({!adjusted_due_date_time_from_GMT} -(10/1440)), 12, 2 ) ) = 0,
    VALUE( MID( TEXT({!adjusted_due_date_time_from_GMT} -(10/1440)), 12, 2 ) ) = 12
  ),
  &quot;12&quot;,
  TEXT( VALUE( MID( TEXT({!adjusted_due_date_time_from_GMT} -(10/1440)), 12, 2 ) ) 
   -
   IF( 
     VALUE( MID( TEXT({!adjusted_due_date_time_from_GMT} -(10/1440)), 12, 2 ) ) &lt; 12, 
     0, 
     12 
   ) 
  )
)
&amp; &quot;:&quot; &amp;
MID( TEXT({!adjusted_due_date_time_from_GMT} -(10/1440)), 15, 2 )

&amp; &quot; &quot; &amp;
IF(
  VALUE( MID( TEXT({!adjusted_due_date_time_from_GMT} -(10/1440)), 12, 2 ) ) &lt; 12,
  &quot;AM&quot;,
  &quot;PM&quot;
)</expression>
